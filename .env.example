# ========================================
# SUPABASE CONFIGURATION
# ========================================
# Get these from your Supabase project settings
# https://supabase.com/dashboard/project/_/settings/api
VITE_APP_SUPABASE_URL=your-supabase-url
VITE_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# ========================================
# MCP SERVER CONFIGURATION
# ========================================
# MCP server URL (for Supabase and Vercel integrations via rube.app)
# Example: http://localhost:3001 or your rube.app MCP endpoint
VITE_MCP_SERVER_URL=http://localhost:3001
VITE_MCP_AUTH_TOKEN=your-mcp-auth-token

# ========================================
# AIRCALL CONFIGURATION
# ========================================
# Aircall API credentials for call data
# Get these from: https://dashboard.aircall.io/integrations/api-keys
VITE_AIRCALL_API_ID=your-aircall-api-id
VITE_AIRCALL_API_TOKEN=your-aircall-api-token

# ========================================
# ELEVENLABS CONFIGURATION
# ========================================
# ElevenLabs API key for bot call analytics
# Get this from: https://elevenlabs.io/app/settings/api-keys
VITE_ELEVENLABS_API_KEY=your-elevenlabs-api-key

# ========================================
# ISN CONFIGURATION
# ========================================
# ISN (Inspection Support Network) credentials
# Get these from your ISN account settings
VITE_ISN_API_KEY=your-isn-api-key
VITE_ISN_COMPANY_ID=your-isn-company-id

# ========================================
# OPTIONAL: VERCEL CONFIGURATION
# ========================================
# If using Vercel direct API (not via MCP)
# VITE_VERCEL_TOKEN=your-vercel-token
# VITE_VERCEL_PROJECT_ID=your-vercel-project-id

# ========================================
# OPTIONAL: OTHER API INTEGRATIONS
# ========================================
# Twilio (alternative to Aircall)
# VITE_TWILIO_ACCOUNT_SID=your-twilio-sid
# VITE_TWILIO_AUTH_TOKEN=your-twilio-auth-token

# Salesforce CRM
# VITE_SALESFORCE_INSTANCE=your-salesforce-instance
# VITE_SALESFORCE_ACCESS_TOKEN=your-salesforce-token

# Custom API base URL (if not using individual APIs)
# VITE_API_BASE_URL=https://your-api.com/api
